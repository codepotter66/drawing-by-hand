{
  "name": "drawing-by-hand",
  "version": "1.0.0",
  "description": "基于MediaPipe的手部追踪画图应用",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build-exe": "pkg . --targets node18-win-x64,node18-macos-x64,node18-linux-x64 --output drawing-app",
    "build-win": "pkg . --targets node18-win-x64 --output drawing-app.exe",
    "build-mac": "pkg . --targets node18-macos-x64 --output drawing-app",
    "build-linux": "pkg . --targets node18-linux-x64 --output drawing-app"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1"
  },
  "keywords": ["mediapipe", "hand-tracking", "drawing", "webcam"],
  "author": "",
  "license": "MIT"
}
